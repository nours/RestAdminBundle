{
    "name": "nours/rest-admin-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Coudrier",
            "email": "david.coudrier@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "psr/container": "~1.0",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.5",
        "jms/serializer-bundle": "^4.0",
        "knplabs/knp-menu-bundle": "^3.2",
        "nours/table-bundle": "^2.0",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^5.4",
        "symfony/doctrine-messenger": "^5.4",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/security-bundle": "^5.4",
        "symfony/twig-bundle": "^5.4"
    },
    "suggest": {
      "nours/table-bundle": "Pagination + table view integration",
      "doctrine/doctrine-bundle" : "Support for Doctrine ORM persistence and loading",
      "symfony/security-bundle": "Integrate security checks based on security bundle",
      "knplabs/knp-menu-bundle": "KnpMenu integration (menu voter)"
    },
    "autoload": {
        "psr-0": { "Nours\\RestAdminBundle": "" }
    },
    "target-dir": "Nours/RestAdminBundle"
}

{
    "name": "nours/rest-admin-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Coudrier",
            "email": "david.coudrier@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.8|~3.0",
        "doctrine/orm" : "~2.5",
        "jms/serializer-bundle": "~1.1",
        "nours/table-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0|~5.0",
        "symfony/phpunit-bridge" : "~3.0",
        "symfony/monolog-bundle" : "~2.11",
        "doctrine/doctrine-bundle" : "~1.4",
        "doctrine/data-fixtures": "~1.1",
        "knplabs/knp-menu-bundle": ">=2.0"

    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/nours/TableBundle"
      }
    ],
    "suggest": {
      "doctrine/doctrine-bundle" : "Support for Doctrine ORM persistence and loading",
      "symfony/security-bundle": "Integrate security checks based on security bundle",
      "knplabs/knp-menu-bundle": "KnpMenu integration (menu voter)"
    },
    "autoload": {
        "psr-0": { "Nours\\RestAdminBundle": "" }
    },
    "target-dir": "Nours/RestAdminBundle"
}